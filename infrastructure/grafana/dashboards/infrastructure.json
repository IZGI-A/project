{
  "uid": "infra-overview",
  "title": "Infrastructure",
  "tags": ["infrastructure", "postgres", "redis", "clickhouse"],
  "timezone": "browser",
  "editable": true,
  "time": { "from": "now-1h", "to": "now" },
  "refresh": "15s",
  "panels": [
    {
      "id": 1,
      "title": "Container CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{id=~\"/docker/[a-f0-9]+\", service!=\"\"}[5m])) by (service)",
          "legendFormat": "{{ service }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 0.8 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Container Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "container_memory_usage_bytes{id=~\"/docker/[a-f0-9]+\", service!=\"\"}",
          "legendFormat": "{{ service }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "PostgreSQL — Active Connections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(pg_stat_activity_count{datname=\"financial_shared\"}) by (state)",
          "legendFormat": "{{ state }}",
          "refId": "A"
        },
        {
          "expr": "pg_settings_max_connections",
          "legendFormat": "max connections",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "max connections" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          }
        ]
      }
    },
    {
      "id": 4,
      "title": "PostgreSQL — Database Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "pg_database_size_bytes{datname=\"financial_shared\"}",
          "legendFormat": "financial_shared",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 536870912 },
              { "color": "red", "value": 1073741824 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "PostgreSQL — Dead Tuples",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(pg_stat_user_tables_n_dead_tup) by (relname)",
          "legendFormat": "{{ relname }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "PostgreSQL — Transactions/sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit{datname=\"financial_shared\"}[5m])",
          "legendFormat": "commits/s",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_xact_rollback{datname=\"financial_shared\"}[5m])",
          "legendFormat": "rollbacks/s",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "rollbacks/s" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "id": 7,
      "title": "ClickHouse — Queries/sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "rate(ClickHouseProfileEvents_Query[5m])",
          "legendFormat": "queries/s",
          "refId": "A"
        },
        {
          "expr": "rate(ClickHouseProfileEvents_SelectQuery[5m])",
          "legendFormat": "selects/s",
          "refId": "B"
        },
        {
          "expr": "rate(ClickHouseProfileEvents_InsertQuery[5m])",
          "legendFormat": "inserts/s",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "title": "ClickHouse — Active Parts",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "ClickHouseMetrics_PartsActive",
          "legendFormat": "active parts",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 200 },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 9,
      "title": "ClickHouse — Memory Usage",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "ClickHouseMetrics_MemoryTracking",
          "legendFormat": "memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 536870912 },
              { "color": "red", "value": 1073741824 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 10,
      "title": "ClickHouse — Merge Operations",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "ClickHouseMetrics_Merge",
          "legendFormat": "active merges",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      }
    },
    {
      "id": 11,
      "title": "Redis — Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "used memory",
          "refId": "A"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "max memory",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "max memory" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          }
        ]
      }
    },
    {
      "id": 12,
      "title": "Redis — Connected Clients",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "clients",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 13,
      "title": "Redis — Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m]))",
          "legendFormat": "hit rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.9 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 14,
      "title": "Redis — Commands/sec",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "commands/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39
}
